{
	"name": "auth",
	"version": "0.0.0",
	"description": "Styles and components for use in svelte websites",
	"type": "module",
	"module": "./src/index.ts",
	"svelte": "./src/index.ts",
	"license": "MIT",
	"scripts": {
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
		"format": "prettier --write --ignore-path=../../.prettierignore ."
	},
	"dependencies": {
		"axios": "^1.5.1",
		"app-globals": "workspace:*"
	},
	"devdependencies": {
		"eslint-config-custom": "workspace:*",
		"svelte": "^4.0.5"
	},
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./src/index.ts",
			"import": "./src/index.ts",
			"types": "./src/index.ts"
		},
		"./persistance": {
			"require": "./src/persistance.ts",
			"import": "./src/persistance.ts",
			"types": "./src/persistance.ts"
		}
	},
	"typesVersions": {
		"*": {
			"persistance": ["./src/persistance.ts"]
		}
	}
}
