{
	"name": "globals",
	"version": "0.0.1",
	"type": "module",
	"module": "./src/index.ts",
	"svelte": "./src/index.ts",
	"license": "MIT",
	"scripts": {
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
		"format": "prettier --write --ignore-path=../../.prettierignore ."
	},
	"devDependencies": {
		"eslint-config-custom": "*",
		"svelte": "^3.9.2"
	},
	"exports": {
		"./*": {
			"require": "./src/*/index.ts",
			"import": "./src/*/index.ts",
			"types": "./src/*/index.ts"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./src/*/index.ts"
			]
		}
	}
}
