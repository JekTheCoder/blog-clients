{
	"useTabs": true,
	"printWidth": 90,
	"arrowParens": "avoid",
	"singleQuote": true,
	"htmlWhitespaceSensitivity": "ignore",
  "plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      },
    },
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			},
		}
  ]
}
